{
  "name": "backend/wordpress",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "wpackagist-plugin/advanced-custom-fields": "6.0.7"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ]
    }
  },
  "config": {
    "allow-plugins": true
  }
}
